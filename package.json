{
  "scripts": {
    "pretest": "yarn tslint",
    "test:fast": "TS_NODE_FILES=true NODE_CONFIG_ENV=test mocha 'src/**/__tests__/**/*.test.{ts,tsx}'",
    "test": "TS_NODE_FILES=true NODE_CONFIG_ENV=test mocha 'src/**/__tests__/**/*.test.{ts,tsx}'",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "clean": "rm -rf build/*",
    "compile": "tsc",
    "watch": "nodemon -x yarn",
    "prepublishOnly": "yarn compile",
    "cover": "nyc mocha 'src/**/__tests__/**/*.test.{ts,tsx}'"
  },
  "nyc": {
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "exclude": [
      "src/test/**/*"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "lcov",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "license": "MIT",
  "keywords": [
    "comparaonline",
    "rules"
  ],
  "name": "@comparaonline/rule-engine",
  "version": "2.2.3",
  "description": "Package to allow rule-based configuration of services",
  "private": false,
  "author": {
    "name": "Ezequiel Rabinovich",
    "email": "erabinovich@gmail.com"
  },
  "nodemonConfig": {
    "execMap": {
      "ts": "ts-node --files"
    },
    "quiet": true,
    "ignore": [
      "build",
      ".git"
    ],
    "ext": "js,json,ts"
  },
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build"
  ],
  "dependencies": {
    "clone-deep": "^4.0.1",
    "moment": "^2.24.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/clone-deep": "^4.0.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.13.7",
    "@types/sinon": "^7.0.11",
    "chai": "^4.2.0",
    "mocha": "^6.1.4",
    "nodemon": "^1.18.11",
    "nyc": "^14.0.0",
    "sinon": "^7.3.2",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.4.4"
  }
}
